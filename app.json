{
  "expo": {
    "name": "Nostra Pizza",
    "slug": "nosstra-pizza",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/nosstra-pizza-logo.png",
    "scheme": "xfefo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/nosstra-pizza-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/nosstra-pizza-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.xfefo.nostrapizza"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/nosstra-pizza-logo.png",
      "name": "Nostra Pizza",
      "title": "Nostra Pizza",
      "description": "Nostra Pizza is a pizza delivery app"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "774dc957-3c0c-4e5c-aff4-facaf3999d08"
      }
    }
  }
}
